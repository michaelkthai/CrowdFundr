<h1>Here is a list of our projects</h1>

<div class="container" style="width: 800px; height: 800px">
  <% @projects.each do |project| %>
  <table class="table">
    <tr>
      <td><p><%= link_to project.title, project_path(project.id) %></p></td>
      <td><img src="http://placekitten.com/400/400"></td>
  </table>
  <% end %>
</div>
